{
  "name": "@codeiqlabs/eslint-config",
  "version": "1.3.0",
  "description": "Shared ESLint v9 flat config for CodeIQLabs projects",
  "type": "module",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./minimal": {
      "types": "./dist/minimal.d.ts",
      "import": "./dist/esm/minimal.js",
      "require": "./dist/cjs/minimal.js"
    },
    "./react": {
      "types": "./dist/react.d.ts",
      "import": "./dist/esm/react.js",
      "require": "./dist/cjs/react.js"
    },
    "./prettier": "./dist/cjs/prettier.config.js",
    "./ignores": {
      "types": "./dist/ignores.d.ts",
      "import": "./dist/esm/ignores.js",
      "require": "./dist/cjs/ignores.js"
    }
  },
  "files": [
    "dist/",
    "README.md",
    ".prettierignore"
  ],
  "sideEffects": false,
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "scripts": {
    "test": "node -e \"console.log('ok')\"",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "changeset": "changeset",
    "clean": "rimraf dist",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:cjs:pkg": "node -e \"require('fs').mkdirSync('dist/cjs',{recursive:true});require('fs').writeFileSync('dist/cjs/package.json','{\\\"type\\\":\\\"commonjs\\\"}\\n')\"",
    "build": "npm run clean && npm run build:esm && npm run build:cjs && npm run build:cjs:pkg",
    "build:cjs:show": "tsc -p tsconfig.cjs.json --showConfig",
    "test:load": "node tests/load-config.test.cjs",
    "prepublishOnly": "npm run build && npm run test:load"
  },
  "keywords": [
    "eslint",
    "prettier",
    "config",
    "flat-config",
    "codeiqlabs"
  ],
  "author": "CodeIQLabs",
  "license": "MIT",
  "engines": {
    "node": ">=18.18"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CodeIQLabs/codeiqlabs-eslint-prettier-config"
  },
  "bugs": {
    "url": "https://github.com/CodeIQLabs/codeiqlabs-eslint-prettier-config/issues"
  },
  "homepage": "https://github.com/CodeIQLabs/codeiqlabs-eslint-prettier-config#readme",
  "peerDependencies": {
    "eslint": "^9.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-hooks": "^4.6.0"
  },
  "peerDependenciesMeta": {
    "eslint-plugin-react": {
      "optional": true
    },
    "eslint-plugin-react-hooks": {
      "optional": true
    }
  },
  "dependencies": {
    "eslint-config-prettier": "^9.1.0",
    "globals": "^15.0.0",
    "prettier": "^3.3.3"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.6",
    "@types/eslint": "^9.6.0",
    "typescript": "^5.4.0",
    "rimraf": "^5.0.0"
  }
}
