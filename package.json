{
  "name": "@codeiqlabs/eslint-config",
  "version": "1.2.0",
  "description": "Shared ESLint v9 flat config for CodeIQLabs projects",
  "main": "index.js",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "exports": {
    ".": "./index.js",
    "./minimal": "./minimal.js",
    "./react": "./react.js",
    "./prettier": "./prettier.config.js"
  },
  "files": [
    "index.js",
    "minimal.js",
    "react.js",
    "prettier.config.js",
    "README.md",
    ".prettierignore"
  ],
  "sideEffects": false,
  "scripts": {
    "test": "node -e \"console.log('ok')\"",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "changeset": "changeset"
  },
  "keywords": [
    "eslint",
    "config",
    "flat-config",
    "codeiqlabs"
  ],
  "author": "CodeIQLabs",
  "license": "MIT",
  "engines": {
    "node": ">=18.18"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CodeIQLabs/codeiqlabs-eslint-config"
  },
  "bugs": {
    "url": "https://github.com/CodeIQLabs/codeiqlabs-eslint-config/issues"
  },
  "homepage": "https://github.com/CodeIQLabs/codeiqlabs-eslint-config#readme",
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^9.0.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "typescript": ">=5.4 <6"
  },
  "peerDependenciesMeta": {
    "eslint-plugin-react": {
      "optional": true
    },
    "eslint-plugin-react-hooks": {
      "optional": true
    }
  },
  "dependencies": {
    "@eslint/js": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^16.3.0",
    "typescript-eslint": "^8.39.1"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.6",
    "prettier": "^3.3.3"
  }
}
